# Environment Variables for SANGAI Backend

# MongoDB Connection
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/sangai?retryWrites=true&w=majority

# Server Configuration
PORT=3001
NODE_ENV=development

# CORS - Frontend URL (comma-separated for multiple)
CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
